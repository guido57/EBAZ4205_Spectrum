#include <stdio.h>
#include "data_capture.h"

double * captured_data_rf_;
int  fft_rf_size_;


int main()
{
    printf("Hello World\n");

    if (captured_data_rf_) delete[] captured_data_rf_;
        captured_data_rf_ = new double[fft_rf_size_];

    Data_Capture *dc = new Data_Capture(0x43C10000, 64, 4096);
    dc->capture_and_read(captured_data_rf_)


    return 0;
}
